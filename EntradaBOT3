create
	int bot bar
		on activation:
			store 3.
		end
	end
	int bot contador
		on default:
			store 35.
		end
	end
	bool bot hola
		on activation:
			collect.
		end
	end
	bool bot hola1
		on activation:
			collect.
		end
	end
	bool bot hola2
		on activation:
			collect.
		end
	end	
execute
	activate contador.
	$- Asignar al contador
	el valor 35, si bar > 2 -$
	if hola = hola1 /\ hola2:
		advance contador.
	end
end